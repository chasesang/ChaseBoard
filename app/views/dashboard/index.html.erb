    <div class="col-lg-3 col-sm-6">
        <section class="panel">
            <div class="symbol red">
                <i class="fa fa-tags"></i>
            </div>
            <div class="value">
                <h1 class=" count2">
                    <%= @user.events.count %>
                </h1>
              <p><%= link_to 'My Events', user_myevents_path(@user) %></p>
            </div>
        </section>
    </div>
    <div class="col-lg-3 col-sm-6">
        <section class="panel">
            <div class="symbol yellow">
                <i class="fa fa-shopping-cart"></i>
            </div>
            <div class="value">
                <h1 class=" count3">
                  21
                </h1>
                <p><%= link_to 'My Documents', user_mydocuments_path(@user) %></p>
