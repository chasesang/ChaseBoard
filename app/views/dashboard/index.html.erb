        </section>
    </div>
    <div class="col-lg-3 col-sm-6">
        <section class="panel">
            <div class="symbol blue">
                <i class="fa fa-bar-chart-o"></i>
            </div>
            <div class="value">
                <h1 class=" count4">
                    12
                </h1>
                <p><%= link_to'My Bookmarks', user_mybookmarks_path(@user) %></p>
            </div>
        </section>
    </div>
</div>
<!--state overview end-->


<section class="wrapper" style="margin-top: 5px">
    <div class="row">
        <header class="panel-heading">
          <center>  <h2>My Todo List</h2> </center>
        </header>
    </div>
    <hr>
    <% if user_signed_in? %>
    <div class="row">
        <div class="col-md-6">
            <section class="panel tasks-widget">
                <header class="panel-heading">
                    Tasks that Assigned To Me
                </header>
                <div class="panel-body">
                    <div class="task-content">
                        <ul id="sortable" class="task-list">
                            <% @user.assigned_tasks.each do |task| %>
                            <% if task.complete? %>
                            <li class="list-primary">

                                <div class="task-title">
                                  <strong>  <span class="task-title-sp"><strike><%= link_to task.body,team_task_path(task.team, task) %></strike></span></strong><br>
                                  From: <span><%= link_to task.team.title,team_path(task.team_id) %></span>
                                    <div class="pull-right hidden-phone">
                                        <button class="btn btn-success btn-xs fa fa-check"></button>
                                        <button class="btn btn-primary btn-xs fa fa-pencil"></button>
                                        <button class="btn btn-danger btn-xs fa fa-trash-o"></button>
                                    </div>
                                </div>
                            </li>

                            <% else %>
                            <li class="list-primary">

                                <div class="task-title">
                                  <strong>  <span class="task-title-sp"><%= link_to task.body,team_task_path(task.team, task) %></span></strong><br>
                                  From: <span><%= link_to task.team.title,team_path(task.team_id) %></span>
                                    <div class="pull-right hidden-phone">
                                        <button class="btn btn-success btn-xs fa fa-check"></button>
                                        <button class="btn btn-primary btn-xs fa fa-pencil"></button>
                                        <button class="btn btn-danger btn-xs fa fa-trash-o"></button>
                                    </div>
                                </div>
                            </li>
                            <% end %>
                            <% end %>
                        </ul>
                    </div>
                    <div class=" add-task-row">
                        <a class="btn btn-success btn-sm pull-left" href="#">Add New Tasks</a>
                        <a class="btn btn-default btn-sm pull-right" href="#">See All Tasks</a>
                    </div>
                </div>
            </section>
        </div>

        <div class="col-md-6">
            <section class="panel tasks-widget">
                <header class="panel-heading">
                    Tasks I Created
                </header>
                <div class="panel-body">
                    <div class="task-content">
                        <ul id="sortable2" class="task-list ui-sortable" style="padding-left: 0px">
                            <% @tasks.each do |task| %>
                            <% if task.complete? %>
                            <li class="list-primary">

                                <div class="task-title">
                                  <strong>  <span class="task-title-sp"><strike><%= link_to task.body,team_task_path(task.team, task) %></strike></span></strong><br>
                                  From: <span><%= link_to task.team.title,team_path(task.team_id) %></span>
                                    <div class="pull-right hidden-phone">
                                        <button class="btn btn-success btn-xs fa fa-check"></button>
                                        <button class="btn btn-primary btn-xs fa fa-pencil"></button>
                                        <button class="btn btn-danger btn-xs fa fa-trash-o"></button>
                                    </div>
                                </div>
                            </li>

                            <% else %>
                            <li class="list-primary">

                                <div class="task-title">
                                  <strong>  <span class="task-title-sp"><%= link_to task.body,team_task_path(task.team, task) %></span></strong><br>
                                  From: <span><%= link_to task.team.title,team_path(task.team_id) %></span>
                                    <div class="pull-right hidden-phone">
                                        <button class="btn btn-success btn-xs fa fa-check"></button>
                                        <button class="btn btn-primary btn-xs fa fa-pencil"></button>
                                        <button class="btn btn-danger btn-xs fa fa-trash-o"></button>
                                    </div>
                                </div>
                            </li>
                            <% end %>
                            <% end %>
                        </ul>
                    </div>
                    <div class=" add-task-row">
                        <a class="btn btn-success btn-sm pull-left" href="#">Add New Tasks</a>
                        <a class="btn btn-default btn-sm pull-right" href="#">See All Tasks</a>
                    </div>
                </div>
            </section>
        </div>
    </div>

    <% end %>
</secion>

<!--Todo content end-->

