      </div>
      <div class="modal-body">
        <%= simple_form_for @newteam do |f| %>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.file_field :documents, multiple: true %>
        <%= f.submit 'Submit', class: 'btn btn-primary' %>
        <% end %>
